#include <stdio.hmm>
#include <stdlib.hmm>
#include <errno.hmm>
#define  BUF 80

char[] buf = array(BUF);

public int main(int argc, char[][] argv) {
  if (argc < 2) {
    printf("realpath: missing operand\n");
    return 1;
  }
  if (absPath(argv[1], buf, BUF)) return errno;
  printf("%s\n", buf);
  return 0;
}
